<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="org.hackday.ksr.infrastructure.mapper.UserAccountMapper">

	<insert id="insert">
		insert into user_account (`email`,`password`,`username`,`phone_num`)
			values (#{email},#{password},#{username},#{phoneNum})

	</insert>

	<select id="selectOne" resultType="UserAccount">
		select 'email','password','name','phone_num'
			from user_account
		where email=#{email}
	</select>










</mapper>